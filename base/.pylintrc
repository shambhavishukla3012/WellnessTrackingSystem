[MASTER]
load-plugins=pylint_django
django-settings-module=settings.local
ignore=migrations

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,abstract-method,unnecessary-pass

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
